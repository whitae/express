{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "html",
        "json"
    ],
    "rules": { 
        "linebreak-style":["error","windows"],
        "indent":["error",4],
        "no-console":[0],
        "no-shadow":[0],
        "newline-per-chained-call":"off",
        "prefer-const":"off",
        "prefer-destructuring":"off",
        "prefer-template":"off",
        "no-underscore-dangle":"off"
    }
}